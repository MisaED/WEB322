<div class="row">
  <div class="col-md-12">
        <h2>Image<a href="/Images/add" class="btn btn-success pull-right">Add New Image</a></h2>

    <hr />
  </div>

  {{#if items.length}}

    {{#each items}}
      <div class="col-md-4">
        <img
          src="/images/uploaded/{{this}}"
          alt="img"
          class="img-responsive img-thumbnail"
        />
      </div>
    {{/each}}

  {{else}}
    <div class="col-md-12 text-center">
      <strong>No Image Available</strong>
    </div>

  {{/if}}
</div>